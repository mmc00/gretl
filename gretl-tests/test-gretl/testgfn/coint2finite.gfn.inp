include ./coint2finite.gfn
open australia

list endo = ius2 iau2

# Bartlett:
coint2finite(endo, 2, 3, 0, null, null)
# Bootstrap:
coint2finite(endo, 2, 3, 500, null, null)

diff e2
lags e2 d_e2
list exo = d_e2 d_e2_1
list rexo = e2_1

coint2finite(endo, 2, 3, 0, exo, rexo)
coint2finite(endo, 2, 3, 500, exo, rexo)